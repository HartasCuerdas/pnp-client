<% title t('Weeks') %>
<% body_classes "pnpC-week pnpC-index" %>
<h1><%= yield :title %></h1>

<p id="notice"><%= notice %></p>

<div class="pnpC-date"><%= t('today_is') + ' ' + @str_today %></div>

<table id="pnpC-weeks-table" class="table">
  <thead>
    <tr>
      <th><%= t 'first_day' %></th>
      <th><%= t 'oAVG' %></th>
      <th><%= t 'dAVG' %></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <%= render @weeks %>
  </tbody>
</table>
